<!ELEMENT SESSION (TRACE)* >
	<!ELEMENT TRACE (TRANSITION)* >
		<!ELEMENT TRANSITION (START_STATE,INPUTS,EVENT,FINAL_STATE)>
			<!ELEMENT START_STATE (DESCRIPTION)>
			<!ELEMENT INPUTS (INPUT)*>
				<!ELEMENT INPUT (WIDGET)>
			<!ELEMENT EVENT (WIDGET)>
			<!ELEMENT FINAL_STATE (DESCRIPTION)>
				<!ELEMENT DESCRIPTION (WIDGET)*>
					<!ELEMENT WIDGET EMPTY>
						
<!ATTLIST SESSION date_time CDATA #IMPLIED>
<!ATTLIST SESSION package_name CDATA #IMPLIED>
